  with
 &lt;denchmark-code&gt;    setup_wandb_logging(
         trainer,
         optimizers=optimizer,
         evaluators={"train_evaluator": train_evaluator, "val_evaluator": validation_evaluator},
     )
 &lt;/denchmark-code&gt;
 
